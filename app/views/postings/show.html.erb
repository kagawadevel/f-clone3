<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @posting.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @posting.content %>
</p>

 <% if @posting.image.present? %>
  <p>
    <strong>Image:</strong>
    <%= image_tag @posting.image.url %>
  </p>
 <% end %>

<%= link_to 'Edit', edit_posting_path(@posting) %>
<%= link_to 'Back', postings_path %>
